    "keywords": ["document", "document management", "DMS", "GED", "document generation"],
    "license": "MIT",
    "authors": [
        {
            "name": "Gabriel Bondaz",
            "email": "gabriel.bondaz@idci-consulting.fr",
            "homepage": "https://www.idci-consulting.fr"
        },
        {
            "name": "Brahim Boukoufallah",
