{
    "name": "idci/document-management-bundle",
    "type": "symfony-bundle",
    "description": "Symfony bundle to handle document management features like storage, pdf generation",
    "keywords": ["document", "document management", "DMS", "GED", "document generation"],
    "license": "MIT",
    "authors": [
        {
            "name": "Gabriel Bondaz",
            "email": "gabriel.bondaz@idci-consulting.fr",
            "homepage": "https://www.idci-consulting.fr"
        },
        {
            "name": "Brahim Boukoufallah",
            "email": "brahim.boukoufallah@idci-consulting.fr",
            "homepage": "https://www.idci-consulting.fr"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "behat/transliterator": "~1.1",
        "h4cc/wkhtmltopdf-amd64": "~0.12",
        "h4cc/wkhtmltopdf-i386": "~0.12",
        "knplabs/knp-snappy-bundle": "~1.5",
        "knplabs/knp-gaufrette-bundle": "~0.5",
        "doctrine/doctrine-bundle": "^1.8",
        "doctrine/orm": "^2.5",
        "ramsey/uuid-doctrine": "~1.4",
        "friendsofsymfony/rest-bundle": "~1.5",
        "jms/serializer-bundle": "^2.3",
        "symfony/options-resolver": "^3.4",
        "symfony/form": "^3.4",
        "twig/twig": "*"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^2.2",
        "phpunit/phpunit": "^5.7",
        "symfony/browser-kit": "^3.4",
        "symfony/framework-bundle": ">=2.8",
        "symfony/twig-bundle": "~3.4",
        "sensio/framework-extra-bundle": "^3.0.2",
        "symfony/templating": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "IDCI\\Bundle\\DocumentManagementBundle\\": ""
        }
    },
    "minimum-stability": "stable"
}
